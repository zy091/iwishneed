{
  "name": "requirement-management-system",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "db:clear": "tsx ./src/scripts/clear-database.ts",
    "db:seed:staff": "tsx ./src/scripts/seed-tech-staff.ts --file=./src/scripts/staff.csv",
